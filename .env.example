# Environment Configuration Example
# Copy this file to .env.local and fill in your values

# Application Settings
NODE_ENV=development
PORT=5173
VITE_APP_ENV=development

# GitHub Container Registry
GITHUB_TOKEN=your_github_token_here
GITHUB_REPOSITORY=omarbizkit/parallel-trail

# Zeabur Deployment
ZEABUR_API_TOKEN=your_zeabur_api_token_here
ZEABUR_PROJECT_ID=your_zeabur_project_id_here
ZEABUR_STAGING_PROJECT_ID=your_zeabur_staging_project_id_here

# Database (if needed later)
# DATABASE_URL=your_database_url_here

# Analytics (optional)
# ANALYTICS_ID=your_analytics_id_here

# Feature Flags
VITE_FEATURE_ANALYTICS=false
VITE_FEATURE_SENTRY=false

# API Endpoints (if needed)
# VITE_API_URL=http://localhost:3001
# VITE_WS_URL=ws://localhost:3001